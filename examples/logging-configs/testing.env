# Testing logging configuration
# Минимальное логирование для тестов

# Основные настройки
LOG_LEVEL=WARNING
ENABLE_FILE_LOGGING=false
ENABLE_JSON_LOGGING=false
LOG_DIR=logs/test

# Размеры и ротация логов
MAX_LOG_SIZE_MB=1
LOG_BACKUP_COUNT=2

# Мониторинг производительности
SLOW_OPERATION_THRESHOLD=1.0
MEMORY_USAGE_THRESHOLD=200
ENABLE_PERFORMANCE_MONITOR=false

# Ollama настройки
OLLAMA_HOST=http://localhost:11434
OLLAMA_DEFAULT_MODEL=qwen2.5vl:latest

# Пути к данным
DATA_PATH=data/test
DOCUMENTS_PATH=data/test/documents
CHROMA_PATH=data/test/chroma_db

# Сессии
SESSION_TIMEOUT_HOURS=1